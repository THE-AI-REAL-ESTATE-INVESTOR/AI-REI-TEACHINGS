{
  "name": "AI-REI-TEACHINGS",
  "version": "1.0.0",
  "description": "AI Learning Hub Automation Tools",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/cli.ts",
    "merge": "ts-node src/cli.ts merge",
    "status": "ts-node src/cli.ts status",
    "commit": "ts-node src/cli.ts commit",
    "help": "ts-node src/cli.ts help",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "NODE_OPTIONS='--loader ts-node/esm' eslint . --ext .ts --config eslint.config.ts",
    "lint:fix": "NODE_OPTIONS='--loader ts-node/esm' eslint . --ext .ts --fix --config eslint.config.ts",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint:fix-all": "NODE_OPTIONS='--loader ts-node/esm' ts-node src/scripts/fix-lint-issues.ts",
    "lint:validate": "NODE_OPTIONS='--loader ts-node/esm' ts-node src/scripts/validate-fixes.ts",
    "lint:fix-enhanced": "NODE_OPTIONS='--loader ts-node/esm' ts-node src/scripts/fix-lint-issues.ts",
    "fix:imports": "tsx src/scripts/fixers/import-fixer.ts",
    "fix:types": "tsx src/scripts/fixers/type-fixer.ts",
    "fix:style": "tsx src/scripts/fixers/style-fixer.ts",
    "fix:all": "tsx src/scripts/fix-all.ts",
    "fix:parse": "tsx src/scripts/fixers/parse-fixer.ts"
  },
  "keywords": [
    "ai",
    "learning",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "@octokit/rest": "^21.1.1",
    "@types/commander": "^2.12.5",
    "@types/glob": "^8.1.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^22.13.14",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "glob": "^11.0.1",
    "marked": "^15.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@typescript-eslint/utils": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.5",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
